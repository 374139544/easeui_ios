#use_frameworks!

platform :ios, '11.0'

workspace 'EaseIMKit.xcworkspace'

target 'EaseIM' do
  platform :ios, '11.0'
pod 'Hyphenate'
pod 'MJRefresh'
pod 'MBProgressHUD'
pod 'Masonry'
project '../../EaseIM/EaseIM/EaseIM.xcodeproj'
end

target 'EaseIMKitDemo' do
  platform :ios, '11.0'
  pod 'Hyphenate'
  pod 'Masonry'
  project '../EaseIMKitDemo/EaseIMKitDemo.xcodeproj'
end

target 'EaseIMKit' do
  project './EaseIMKit.xcodeproj'
  pod 'Hyphenate'
  pod 'EMVoiceConvert', '~> 0.1.0'
end


